    "9c122172-8945-4fa7-acb2-3f37d16af7b9": {
      "type": "lookbook-section-1",
      "blocks": {
        "template--16896431325398__9c122172-8945-4fa7-acb2-3f37d16af7b9-16838313647c0b541a-0": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/Floor_Lamps_Mirrors_45_e4cdba59-1180-4955-838b-d8de93fd5c16.jpg",
            "text": "<p><em>One of several maquettes for a brick fireplace that Josef Albers designed for a house in Bethany, CT.<\/em><\/p>",
