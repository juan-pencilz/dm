    "727e0a25-236e-4b9f-bc68-682507b60924": {
      "type": "lookbook-section-1",
      "blocks": {
        "template--16896431325398__727e0a25-236e-4b9f-bc68-682507b60924-16838826679f88d577-2": {
          "type": "column",
          "settings": {
